# Create a copy "build.properties" of this file and change the properties

# API repository URL 
api.url=http://{Host}/api

# Ivy repository URL 
ivy.url=http://{Host}/ivy

# (linux-amd64, linux-arm, win32-x86-64)
platform=linux-amd64

# compile with debug information
javac.debug=on

#skip unit tests before publish
skip.test=false